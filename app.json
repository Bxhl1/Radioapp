{
  "expo": {
    "name": "WesleyRadio",
    "slug": "WesleyRadio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./project-root/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./project-root/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./project-root/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.RadioApp.RadioApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
